    spark_c_spectral_seed_t -> spark_f_spectral_seed_t [label = "OUT"; color = darkred; comment = ""; dir = both;];
    spark_d_spectral_seed_t -> spark_f_spectral_seed_t [label = "OUT"; color = darkgreen; comment = ""; dir = both;];
