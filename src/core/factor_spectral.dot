    spark_d_spectral_seed_t [label = "Spectral.commit()";comment = "";];
    spark_e_spectral_seed_t [label = "Spectral.serve()";comment = "";];
    spark_f_spectral_seed_t [label = "Spectral.exec()";comment = "";];
    
    // IN = Input
    // OUT = Output
    // REC = Recursion
    spark_0_spectral_seed_t -> spark_a_spectral_seed_t [label = "IN"; color = darkred; comment = "";];
    spark_0_spectral_seed_t -> spark_b_spectral_seed_t [label = "OUT"; color = darkred; comment = "";];
