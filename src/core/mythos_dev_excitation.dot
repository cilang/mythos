    spark_d_excitation_t -> spark_f_excitation_t [label = "OUT"; color = darkgreen; comment = ""; dir = both;];
    spark_e_excitation_t -> spark_f_excitation_t [label = "OUT"; color = darkblue; comment = ""; dir = both;];
