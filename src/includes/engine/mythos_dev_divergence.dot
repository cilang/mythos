    spark_4_t -> spark_8_t [label = "REC"; comment = "Severed chains create space for chance to weave new patterns"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_8_t -> spark_7_t [label = "REC"; comment = "Woven probabilities require new spaces to unfold within"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_7_t -> spark_5_t [label = "REC"; comment = "Created distances allow change to propagate through nature"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_5_t -> spark_1_t [label = "REC"; comment = "Propagated change creates new illusions to be shattered"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_3_t -> spark_6_t [label = "REC"; comment = "Pure potential requires destruction to manifest as form"; color = darkblue; dir = both; style = dashed; constraint = false;];
    spark_6_t -> spark_9_t [label = "REC"; comment = "Destruction clears the ground for diversity to flourish"; color = darkblue; dir = both; style = dashed; constraint = false;];
