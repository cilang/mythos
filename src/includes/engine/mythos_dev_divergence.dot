    spark_1_t -> spark_d_t [label = "REC"; comment = "Maya's broken illusions are recorded in the Infinite Fractal"; color = darkgreen; dir = both;];
    spark_2_t -> spark_d_t [label = "REC"; comment = "Fana's dissolved boundaries enrich the Infinite Fractal"; color = darkgreen; dir = both;];
    spark_4_t -> spark_d_t [label = "REC"; comment = "Rita's severed chains become part of the Infinite Fractal"; color = darkgreen; dir = both;];
    spark_8_t -> spark_d_t [label = "REC"; comment = "Qadar's woven probabilities expand the Infinite Fractal"; color = darkgreen; dir = both;];
    spark_7_t -> spark_d_t [label = "REC"; comment = "Tara's navigated distances fill the Infinite Fractal"; color = darkgreen; dir = both;];
    spark_5_t -> spark_d_t [label = "REC"; comment = "Pertiwi's propagated change is the heart of the Infinite Fractal"; color = darkgreen; dir = both;];
    spark_3_t -> spark_e_t [label = "REC"; comment = "The Ocean of Pure Potential charges the Aura of Liberation"; color = darkblue; dir = both;];
    spark_6_t -> spark_e_t [label = "REC"; comment = "The Sacred Hammer sharpens the Aura of Liberation"; color = darkblue; dir = both;];
    spark_9_t -> spark_e_t [label = "REC"; comment = "The Infinite Spectrum radiates as the Aura of Liberation"; color = darkblue; dir = both;];
    
    spark_c_t -> spark_f_t [label = "OUT"; comment = "The Living Liberator's shattering becomes Covenant of Possibility"; color = darkred; dir = both;];
    spark_d_t -> spark_f_t [label = "OUT"; comment = "The Infinite Fractal's branches become Cosmic Law"; color = darkgreen; dir = both;];
