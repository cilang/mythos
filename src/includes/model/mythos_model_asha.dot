strict digraph AshaModel {
    style = filled;
    color = lightgray;
    node [shape = circle; style = filled; color = lightgreen;];
    edge [color = darkgray;];
    label = "Asha Model (/proc/model/asha)";
    comment = "The truth and cosmic order framework. It defines the principles of divine truth, moral order, and epistemological certainty—the Zoroastrian concept of Asha as the fundamental cosmic principle that governs reality, knowledge, and righteous conduct in alignment with universal truth.";
    
    spark_0_asha_model_t [label = "Asha.meta(Asha)";comment = "Abstract: The Cosmic Order and Divine Truth. The fundamental principle of reality's inherent truthfulness and moral order—the Zoroastrian concept of Asha representing cosmic harmony, divine wisdom, and the ultimate reality that underpins all existence and knowledge.";shape = doublecircle;color = darkgray;];
    spark_1_asha_model_t [label = "Asha.r1(Insight)";comment = "Initiation: The Flash of Understanding. The sudden intuitive apprehension of truth—the moment of revelation where hidden patterns become clear and fundamental truths reveal themselves to the prepared mind through divine inspiration.";color = darkgreen;];
    spark_2_asha_model_t [label = "Asha.r2(Discernment)";comment = "Response: The Separation of Truth from Falsehood. The careful analytical process of distinguishing genuine truth from deception—the critical evaluation of information that prevents confusion and maintains clarity in understanding.";color = darkgreen;];
    spark_4_asha_model_t [label = "Asha.r4(Comprehension)";comment = "Integration: The Holistic Understanding. The complete grasp of truth in its full context—the synthesis of insights into coherent understanding that reveals the interconnected nature of reality and moral principles.";color = darkgreen;];
    spark_8_asha_model_t [label = "Asha.r8(Clarity)";comment = "Reflection: The Pure Transparency of Truth. The unobstructed perception of reality as it truly is—the state of mental purity where truth shines forth without distortion, ambiguity, or subjective coloring.";color = darkgreen;];
    spark_7_asha_model_t [label = "Asha.r7(Integrity)";comment = "Consolidation: The Wholeness and Consistency of Truth. The alignment between thought, word, and action with cosmic order—the state where one's entire being reflects and embodies truth without contradiction or hypocrisy.";color = darkgreen;];
    spark_5_asha_model_t [label = "Asha.r5(Sincerity)";comment = "Propagation: The Genuine Expression of Truth. The authentic communication and manifestation of understanding—the truthful expression that spreads clarity and builds trust through honest, heartfelt sharing.";color = darkgreen;];
    spark_3_asha_model_t [label = "Asha.r3(Conviction)";comment = "Thesis: The Unshakable Certainty in Truth. The firm foundation of belief based on direct experience and rational understanding—the bedrock certainty that withstands doubt and provides stability in the pursuit of truth.";color = darkblue;];
    spark_6_asha_model_t [label = "Asha.r6(Judgement)";comment = "Antithesis: The Critical Evaluation of Truth Claims. The careful weighing of evidence and reasoning—the discerning assessment that tests truth against reality and separates valid understanding from error through rigorous examination.";color = darkblue;];
    spark_9_asha_model_t [label = "Asha.r9(Principle)";comment = "Synthesis: The Universal Truths Derived from Experience. The fundamental laws and ethical guidelines that emerge from tested understanding—the enduring truths that guide right thought, right speech, and right action across all contexts.";color = darkblue;];
    spark_a_asha_model_t [label = "Asha.receive(Truth)";comment = "Potential: The unverified, raw data of reality. The fundamental facts and patterns of existence as they are—the objective reality waiting to be understood, before interpretation or application transforms it into wisdom.";shape = invtriangle;color = darkred;];
    spark_b_asha_model_t [label = "Asha.send(Wisdom)";comment = "Manifest: The contextual, applied understanding of truth. The practical knowledge gained through experience and reflection—the living understanding that guides effective action and produces beneficial results in specific situations.";shape = triangle;color = darkred;];
    spark_c_asha_model_t [label = "Asha.dispatch(Adjudication)";comment = "Why-Who: The Authority and Process of Truth Determination. The legitimate methods and qualified individuals who establish truth—the judicial and epistemological processes that fairly evaluate evidence and render authoritative judgments about reality.";shape = doublecircle;color = darkred;];
    spark_d_asha_model_t [label = "Asha.commit(Covenant)";comment = "What-How: The Sacred Agreement to Uphold Truth. The binding commitment to truthfulness and moral order—the formal promises and ethical contracts that establish trust and maintain cosmic harmony through faithful adherence to principles.";shape = doublecircle;color = darkgreen;];
    spark_e_asha_model_t [label = "Asha.serve(Guidance)";comment = "When-Where: The Contextual Application of Truth. The timely and appropriate application of understanding—the wise direction that helps navigate specific circumstances while remaining faithful to universal principles of truth and order.";shape = doublecircle;color = darkblue;];
    spark_f_asha_model_t [label = "Asha.exec(Credibility)";comment = "Which-Closure: The Established Trust in Truth. The proven reliability and trustworthiness earned through consistent truthfulness—the reputation for integrity that makes one's words and actions believable and influential in the community.";shape = doublecircle;color = lightgray;];
    
