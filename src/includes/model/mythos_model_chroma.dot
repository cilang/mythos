    label = "Chroma Model (/proc/model/chroma)";
    comment = "The color and quantum charge framework. It defines the principles of chromatic perception, color theory, and quantum chromodynamics—bridging the visual spectrum with fundamental particle physics through the mathematics of color spaces and quantum field theory.";
    
    spark_0_chroma_model_t [label = "Chroma.meta(Chroma)";comment = "Abstract: The Primordial Color Field. The fundamental quality-space where all hues, saturations, and color charges exist as potential—the quantum field of chromatic possibilities that underlies both visual perception and strong nuclear interactions.";shape = doublecircle;color = darkgray;];
    spark_1_chroma_model_t [label = "Chroma.r1(Red)";comment = "Initiation: Intensity, Action, Vitality. The long-wavelength end of the spectrum representing primal energy, quantum chromodynamics' red charge, and the psychological impact of urgency, passion, and fundamental excitation states.";color = darkgreen;];
    spark_2_chroma_model_t [label = "Chroma.r2(Blue)";comment = "Response: Depth, Stability, Truth. The short-wavelength domain embodying calm authority, quantum blue charge, and the qualities of depth, fidelity, and stable structural relationships in both visual and quantum contexts.";color = darkgreen;];
    spark_4_chroma_model_t [label = "Chroma.r4(Green)";comment = "Integration: Balance, Growth, Life. The middle spectrum representing harmony, quantum green charge (in RGB context), and the balancing force between extremes—mediating relationships and enabling sustainable development.";color = darkgreen;];
    spark_8_chroma_model_t [label = "Chroma.r8(Cyan)";comment = "Reflection: Clarity, Healing, Flow. The additive secondary color combining blue and green—representing transparency, fluid communication, and the clear transmission of information through both light and quantum channels.";color = darkgreen;];
    spark_7_chroma_model_t [label = "Chroma.r7(Magenta)";comment = "Consolidation: Fusion, Harmony, Spirit. The non-spectral color bridging red and blue—symbolizing quantum superposition states, spiritual synthesis, and the unification of apparent opposites through higher-dimensional understanding.";color = darkgreen;];
    spark_5_chroma_model_t [label = "Chroma.r5(Yellow)";comment = "Propagation: Intellect, Joy, Energy. The bright primary combining red and green—representing luminous intelligence, radiant propagation, and the cheerful dissemination of ideas and quantum information.";color = darkgreen;];
    spark_3_chroma_model_t [label = "Chroma.r3(ZeroChroma)";comment = "Thesis: This is color charge neutrality. The QCD 'white'/colorless state. The potential for color, not 'black'. Potential, Void, Absorption. The ground state of the color field where all charges cancel—the quantum vacuum awaiting excitation and manifestation.";color = darkblue;];
    spark_6_chroma_model_t [label = "Chroma.r6(FullChroma)";comment = "Antithesis: This is the maximum additive synthesis. The pure, saturated RGB white light. The ideal subtractive 'white' (the canvas), not 'white'. Purity, Unity, Synthesis. The complete excitation state where all color potentials are simultaneously actualized at maximum intensity.";color = darkblue;];
    spark_9_chroma_model_t [label = "Chroma.r9(Gray)";comment = "Synthesis: This is the perceptual balance point. The midpoint between ZeroChroma and FullChroma. The result of imperfect mixing. Neutrality, Balance, Uncertainty. The quantum mixed state representing probabilistic distributions and the uncertainty inherent in color perception and measurement.";color = darkblue;];
    spark_a_chroma_model_t [label = "Chroma.receive(Golden)";comment = "Potential: The potential for all qualities. Value, Purity. The golden ratio proportion in color space—the ideal harmonious relationships and perfect ratios that define aesthetic beauty and quantum symmetry principles before manifestation.";shape = invtriangle;color = darkred;];
    spark_b_chroma_model_t [label = "Chroma.send(Silver)";comment = "Manifest: The manifested, specific quality. Reflective, expression. The actualized color property with specific wavelength and intensity—the reflective surface that shows color in context and expresses quantum states through measurable phenomena.";shape = triangle;color = darkred;];
    spark_c_chroma_model_t [label = "Chroma.dispatch(Charge)";comment = "Why-Who: Dispatches a specific color charge. Routes to 'Obsidian' (dense, bound). The assignment of quantum chromodynamics properties to particles—determining which quarks carry which color charges and how they interact through gluon exchange in strong nuclear interactions.";shape = doublecircle;color = darkred;];
    spark_d_chroma_model_t [label = "Chroma.commit(Pigment)";comment = "What-How: Commits a pigment to a medium (Color Theory action). Routes to 'Milky' (diffuse, scattering). The physical instantiation of color through material properties—how specific wavelengths are absorbed, reflected, or transmitted by substances to create perceived colors.";shape = doublecircle;color = darkgreen;];
    spark_e_chroma_model_t [label = "Chroma.serve(HSB)";comment = "When-Where: Serves a perceptual hue, saturation, brightness to an observer. Amethyst(the perceptual experience of violet, the highest frequency). The human perceptual color space—delivering specific hue, saturation, and brightness combinations to conscious observers in particular contexts and lighting conditions.";shape = doublecircle;color = darkblue;];
    spark_f_chroma_model_t [label = "Chroma.exec(Chromatic)";comment = "Which-Closure: The Complete Color Manifestation. The full resolution of color properties across all contexts—the harmonious integration of quantum charges, visual perceptions, and material implementations into a coherent chromatic reality that satisfies all color constraints simultaneously.";shape = doublecircle;color = lightgray;];
    
