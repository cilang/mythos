strict digraph KarsaModel {
    style = filled;
    color = lightgray;
    node [shape = circle; style = filled; color = lightgreen;];
    edge [color = darkgray;];
    label = "Karsa Model (/proc/model/karsa)";
    comment = "The will and spacetime manifestation framework. It defines the principles of conscious intention, temporal flow, and spatial extension—the Javanese concept of Karsa representing the fundamental creative will that bridges consciousness with physical reality through the medium of spacetime.";
    
    spark_0_karsa_model_t [label = "Karsa.meta(Karsa)";comment = "Abstract: The Primordial Creative Will. The fundamental force of intention and desire that precedes manifestation—the Javanese concept of Karsa as the divine will that initiates all creation, the conscious impulse that shapes reality through focused intention across spacetime.";shape = doublecircle;color = darkgray;];
    spark_1_karsa_model_t [label = "Karsa.r1(Rhythm)";comment = "Initiation: The Pulsating Flow of Time. The fundamental cadence of existence—the cyclical patterns, heartbeats, and oscillatory motions that provide the temporal structure for will to unfold and intentions to manifest through repeated patterns.";color = darkgreen;];
    spark_2_karsa_model_t [label = "Karsa.r2(Inertia)";comment = "Response: The Resistance to Change. The tendency of existing states to persist—the quality of matter and consciousness that maintains current conditions until sufficient will is applied to overcome resistance and initiate new motions.";color = darkgreen;];
    spark_4_karsa_model_t [label = "Karsa.r4(Temporal)";comment = "Integration: The Arrow of Time Experience. The subjective flow of past-present-future—the continuum through which will operates, where intentions form in the present, draw from the past, and project into future manifestations.";color = darkgreen;];
    spark_8_karsa_model_t [label = "Karsa.r8(Pattern)";comment = "Reflection: The Recurring Structures of Reality. The recognizable forms and sequences that emerge from will—the archetypal templates, habits, and cosmic blueprints that guide manifestation through established pathways.";color = darkgreen;];
    spark_7_karsa_model_t [label = "Karsa.r7(Momentum)";comment = "Consolidation: The Accumulated Force of Will. The building power of sustained intention—the gathered energy of focused desire that overcomes obstacles and carries manifestations forward through spacetime with increasing certainty.";color = darkgreen;];
    spark_5_karsa_model_t [label = "Karsa.r5(Spatial)";comment = "Propagation: The Extension Through Space. The dimensional framework for manifestation—the physical and metaphysical domains where will takes form, extending intentions across distances and through different realms of existence.";color = darkgreen;];
    spark_3_karsa_model_t [label = "Karsa.r3(Birth)";comment = "Thesis: The Emergence of New Forms. The beginning point of manifestation—the moment when potential becomes actual, when will crystallizes into form and new existence emerges into spacetime reality.";color = darkblue;];
    spark_6_karsa_model_t [label = "Karsa.r6(Death)";comment = "Antithesis: The Dissolution of Forms. The necessary ending of manifestations—the transition point where forms release back to potential, making space for new creations and completing cycles of will expression.";color = darkblue;];
    spark_9_karsa_model_t [label = "Karsa.r9(Life)";comment = "Synthesis: The Sustained Expression of Will. The continuous flow between birth and death—the dynamic equilibrium where will maintains forms, adapts to changes, and expresses creatively through the ongoing dance of existence.";color = darkblue;];
    spark_a_karsa_model_t [label = "Karsa.receive(Fixation)";comment = "Potential: The potential for a point to be defined in the field. The moment of focused intention—the precise localization of will where consciousness anchors itself in spacetime, creating reference points for manifestation to begin.";shape = invtriangle;color = darkred;];
    spark_b_karsa_model_t [label = "Karsa.send(Excitation)";comment = "Manifest: The propagated wave/particle through Spacetime. The dynamic expression of will—the energy and information that radiates from points of fixation, carrying intention across the fabric of reality as both wave-like possibilities and particle-like actualizations.";shape = triangle;color = darkred;];
    spark_c_karsa_model_t [label = "Karsa.dispatch(Initiation)";comment = "Why-Who: The Source and Agent of Will. The conscious beings and cosmic forces that generate intention—the specific entities, consciousness streams, and divine agencies that originate creative impulses and determine why manifestations begin and who initiates them.";shape = doublecircle;color = darkred;];
    spark_d_karsa_model_t [label = "Karsa.commit(Continuation)";comment = "What-How: The Sustained Process of Manifestation. The ongoing maintenance and development of created forms—the specific methods and persistent efforts that keep manifestations alive, growing, and evolving according to original intentions.";shape = doublecircle;color = darkgreen;];
    spark_e_karsa_model_t [label = "Karsa.serve(Reincarnation)";comment = "When-Where: The Cyclical Return of Will. The appropriate timing and placement of manifestations—the cosmic recycling process where essences return in new forms, learning and evolving through multiple lifetimes and contexts of expression.";shape = doublecircle;color = darkblue;];
    spark_f_karsa_model_t [label = "Karsa.exec(Spatiotemporal)";comment = "Which-Closure: The Complete Manifestation in Reality. The full integration of will with spacetime—the successful embodiment of intention within the space-time continuum where desire becomes concrete reality with specific location, duration, and material presence.";shape = doublecircle;color = lightgray;];
    
    spark_a_karsa_model_t -> spark_0_karsa_model_t [label = "IN"; comment = "Fixation anchors Creative Will (Karsa) in reality"; color = darkred; constraint = false;];
