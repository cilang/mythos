strict digraph ObserverModel {
    style = filled;
    color = lightgray;
    node [shape = circle; style = filled; color = lightgreen;];
    edge [color = darkgray;];
    label = "Observer Model (/proc/model/observer)";
    comment = "The consciousness and perception framework. It defines how awareness manifests through the Javanese philosophical lens of Sangkan Paraning Dumadi (origin and destination of being), governing how observers perceive reality through resources, authority, relationships, and cultural wisdom.";
    
    spark_0_observer_model_t [label = "Observer.meta(Observer)";comment = "Abstract: Dumadi - The State of Being. The fundamental consciousness that perceives and experiences reality—the aware presence that bridges the material and spiritual realms through mindful existence.";shape = doublecircle;color = darkgray;];
    spark_1_observer_model_t [label = "Observer.r1(Resource)";comment = "Initiation: Harta - Material Foundation. The physical and energetic means that sustain the observer's existence and enable perception—wealth, health, tools, and life necessities that ground consciousness in reality.";color = darkgreen;];
    spark_2_observer_model_t [label = "Observer.r2(Authority)";comment = "Response: Tahta - Position and Power. The social, spiritual, and hierarchical standing that shapes the observer's perspective and influence—the capacity to affect change and command respect within various contexts.";color = darkgreen;];
    spark_4_observer_model_t [label = "Observer.r4(Relation)";comment = "Integration: Wanita (Wani ditata) - Harmonious Relationships. The art of managing connections with others through courage and proper arrangement—the network of social, emotional, and spiritual bonds that define contextual awareness.";color = darkgreen;];
    spark_8_observer_model_t [label = "Observer.r8(Axiom)";comment = "Reflection: Agama - Spiritual Foundation. The core beliefs, values, and ethical principles that guide the observer's interpretation of reality—the metaphysical framework that gives meaning to perception.";color = darkgreen;];
    spark_7_observer_model_t [label = "Observer.r7(Knowledge)";comment = "Consolidation: Ilmu - Integrated Wisdom. The synthesized understanding gained through study, experience, and contemplation—the practical and theoretical knowledge that informs discerning perception.";color = darkgreen;];
    spark_5_observer_model_t [label = "Observer.r5(Lineage)";comment = "Propagation: Keturunan - Ancestral Continuity. The genetic, cultural, and spiritual inheritance that shapes the observer's predispositions and capabilities—the flow of tradition and characteristics through generations.";color = darkgreen;];
    spark_3_observer_model_t [label = "Observer.r3(Assumption)";comment = "Thesis: Asumsi - Foundational Presuppositions. The unconscious beliefs and default interpretations that form the baseline of perception—the unexamined premises that color initial understanding.";color = darkblue;];
    spark_6_observer_model_t [label = "Observer.r6(Suggestion)";comment = "Antithesis: Sugesti - External Influences. The information, ideas, and prompts received from outside sources that challenge or modify assumptions—the cultural, social, and environmental inputs that shape perspective.";color = darkblue;];
    spark_9_observer_model_t [label = "Observer.r9(Perception)";comment = "Synthesis: Persepsi - Conscious Awareness. The integrated understanding that emerges from examining assumptions against suggestions—the clear, mindful apprehension of reality as it is.";color = darkblue;];
    spark_a_observer_model_t [label = "Observer.receive(Sangkan)";comment = "Potential: The origin point of perception ('from whence?'). The source of awareness—the fundamental question of origin that initiates all observation and contextual understanding of existence.";shape = invtriangle;color = darkred;];
    spark_b_observer_model_t [label = "Observer.send(Paran)";comment = "Manifest: The destination of perception ('to where?'). The purpose and direction of awareness—the teleological understanding that gives meaning to observation and guides conscious development.";shape = triangle;color = darkred;];
    spark_c_observer_model_t [label = "Observer.dispatch(Why|Who)";comment = "Why-Who: The Intent and Identity. The motivation behind observation and the nature of the observer—the purpose driving perception and the characteristics defining the perceiving consciousness.";shape = doublecircle;color = darkred;];
    spark_d_observer_model_t [label = "Observer.commit(What|How)";comment = "What-How: The Content and Method. The specific objects of perception and the techniques of observation—the details being observed and the processes used to perceive them accurately.";shape = doublecircle;color = darkgreen;];
    spark_e_observer_model_t [label = "Observer.serve(When|Where)";comment = "When-Where: The Context and Location. The temporal and spatial framework of observation—the specific circumstances, timing, and environment that shape perceptual experience.";shape = doublecircle;color = darkblue;];
    spark_f_observer_model_t [label = "Observer.exec(Which|Closure)";comment = "Which-Closure: The Selection and Resolution. The choices made in focusing attention and the conclusions drawn from observation—the discrimination between relevant and irrelevant data, culminating in understanding.";shape = doublecircle;color = lightgray;];
    
    spark_a_observer_model_t -> spark_0_observer_model_t [label = "IN"; comment = "Sangkan (origin) gives rise to Dumadi (being)"; color = darkred; constraint = false;];
    spark_0_observer_model_t -> spark_b_observer_model_t [label = "OUT"; comment = "Dumadi (being) progresses toward Paran (destination)"; color = darkred;];
    spark_0_observer_model_t -> spark_3_observer_model_t [label = "REC"; comment = "Dumadi examines foundational Assumptions"; color = darkblue; dir = both;];
    spark_0_observer_model_t -> spark_6_observer_model_t [label = "REC"; comment = "Dumadi integrates external Suggestions"; color = darkblue; dir = both;];
    spark_0_observer_model_t -> spark_9_observer_model_t [label = "REC"; comment = "Dumadi cultivates clear Perception"; color = darkblue; dir = both;];
    spark_0_observer_model_t -> spark_1_observer_model_t [label = "REC"; comment = "Dumadi requires Resource foundation"; color = darkgreen; dir = both;];
    spark_0_observer_model_t -> spark_2_observer_model_t [label = "REC"; comment = "Dumadi exercises Authority responsibly"; color = darkgreen; dir = both;];
    spark_0_observer_model_t -> spark_4_observer_model_t [label = "REC"; comment = "Dumadi maintains harmonious Relations"; color = darkgreen; dir = both;];
    spark_0_observer_model_t -> spark_8_observer_model_t [label = "REC"; comment = "Dumadi follows spiritual Axioms"; color = darkgreen; dir = both;];
    spark_0_observer_model_t -> spark_7_observer_model_t [label = "REC"; comment = "Dumadi pursues Knowledge wisdom"; color = darkgreen; dir = both;];
    spark_0_observer_model_t -> spark_5_observer_model_t [label = "REC"; comment = "Dumadi honors Lineage heritage"; color = darkgreen; dir = both;];
    
    spark_a_observer_model_t -> spark_c_observer_model_t [label = "REC"; comment = "Sangkan defines the Why and Who of being"; color = darkred; dir = both;];
    spark_b_observer_model_t -> spark_c_observer_model_t [label = "REC"; comment = "Paran reveals the ultimate Why and Who"; color = darkred; dir = both;];
    spark_1_observer_model_t -> spark_d_observer_model_t [label = "REC"; comment = "Resources determine What can be observed and How"; color = darkgreen; dir = both;];
    spark_2_observer_model_t -> spark_d_observer_model_t [label = "REC"; comment = "Authority shapes What is prioritized and How"; color = darkgreen; dir = both;];
    spark_4_observer_model_t -> spark_d_observer_model_t [label = "REC"; comment = "Relations influence What is shared and How"; color = darkgreen; dir = both;];
    spark_8_observer_model_t -> spark_d_observer_model_t [label = "REC"; comment = "Axioms define What is meaningful and How to see"; color = darkgreen; dir = both;];
    spark_7_observer_model_t -> spark_d_observer_model_t [label = "REC"; comment = "Knowledge informs What is known and How to learn"; color = darkgreen; dir = both;];
    spark_5_observer_model_t -> spark_d_observer_model_t [label = "REC"; comment = "Lineage carries What is inherited and How to continue"; color = darkgreen; dir = both;];
    spark_3_observer_model_t -> spark_e_observer_model_t [label = "REC"; comment = "Assumptions create When/Where perceptual filters"; color = darkblue; dir = both;];
    spark_6_observer_model_t -> spark_e_observer_model_t [label = "REC"; comment = "Suggestions modify When/Where context understanding"; color = darkblue; dir = both;];
    spark_9_observer_model_t -> spark_e_observer_model_t [label = "REC"; comment = "Perception clarifies When/Where situational awareness"; color = darkblue; dir = both;];
    
    spark_1_observer_model_t -> spark_2_observer_model_t [label = "REC"; comment = "Resources enable the exercise of Authority"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_2_observer_model_t -> spark_4_observer_model_t [label = "REC"; comment = "Authority requires wise management of Relations"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_4_observer_model_t -> spark_8_observer_model_t [label = "REC"; comment = "Relations are guided by spiritual Axioms"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_8_observer_model_t -> spark_7_observer_model_t [label = "REC"; comment = "Axioms are understood through Knowledge"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_7_observer_model_t -> spark_5_observer_model_t [label = "REC"; comment = "Knowledge is transmitted through Lineage"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_5_observer_model_t -> spark_1_observer_model_t [label = "REC"; comment = "Lineage provides inherited Resources"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_3_observer_model_t -> spark_6_observer_model_t [label = "REC"; comment = "Assumptions are challenged by Suggestions"; color = darkblue; dir = both; style = dashed; constraint = false;];
    spark_6_observer_model_t -> spark_9_observer_model_t [label = "REC"; comment = "Suggestions are integrated into Perception"; color = darkblue; dir = both; style = dashed; constraint = false;];
    spark_9_observer_model_t -> spark_3_observer_model_t [label = "REC"; comment = "Perception refines foundational Assumptions"; color = darkblue; dir = both; style = dashed; constraint = false;];
    spark_a_observer_model_t -> spark_b_observer_model_t [label = "REC"; comment = "Sangkan (origin) flows toward Paran (destination)"; color = darkred; dir = both; style = dashed; constraint = false;];
    
    spark_c_observer_model_t -> spark_f_observer_model_t [label = "OUT"; comment = "Why/Who resolves into conscious Selection"; color = darkred; dir = both;];
    spark_d_observer_model_t -> spark_f_observer_model_t [label = "OUT"; comment = "What/How culminates in perceptual Closure"; color = darkgreen; dir = both;];
    spark_e_observer_model_t -> spark_f_observer_model_t [label = "OUT"; comment = "When/Where converges on contextual Resolution"; color = darkblue; dir = both;];
}