strict digraph RasaModel {
    style = filled;
    color = lightgray;
    node [shape = circle; style = filled; color = lightgreen;];
    edge [color = darkgray;];
    label = "Rasa Model (/proc/model/rasa)";
    comment = "The emotional essence and aesthetic experience framework. It defines the principles of feeling, emotional alchemy, and artistic expression—the Sanskrit concept of Rasa representing the essential juice or flavor of emotional experience that arises from artistic creation and spiritual practice.";
    
    spark_0_t [label = "Rasa.meta(Rasa)";comment = "Abstract: The Essential Juice of Experience. The fundamental principle of aesthetic and emotional essence—the Sanskrit concept of Rasa as the distilled flavor of consciousness, the pure emotional resonance that arises when subject and object merge in profound aesthetic or spiritual experience.";shape = doublecircle;color = darkgray;];
    spark_1_t [label = "Rasa.r1(Will)";comment = "Initiation: The Conscious Direction of Energy. The focused intention that channels emotional energy—the deliberate steering of instinctual drives toward conscious goals, transforming raw desire into purposeful action through disciplined direction.";color = darkgreen;];
    spark_2_t [label = "Rasa.r2(Instinct)";comment = "Response: The Primal Biological Drive. The innate, automatic patterns of behavior—the fundamental survival impulses, biological compulsions, and deep-seated urges that form the raw material for emotional and spiritual transformation.";color = darkgreen;];
    spark_4_t [label = "Rasa.r4(Drift)";comment = "Integration: The Natural Flow and Surrender. The state of allowing and non-resistance—the graceful acceptance of emotional currents, the willingness to be carried by larger forces, and the integration of will with natural rhythms.";color = darkgreen;];
    spark_8_t [label = "Rasa.r8(Intuition)";comment = "Reflection: The Direct Knowing Beyond Reason. The immediate apprehension of truth—the gut feeling, spiritual insight, and non-rational understanding that emerges from the depths of consciousness without logical processing.";color = darkgreen;];
    spark_7_t [label = "Rasa.r7(Expression)";comment = "Consolidation: The Outward Manifestation of Inner State. The artistic and communicative release—the creative outpouring, emotional display, and authentic sharing of inner experience through art, speech, movement, and presence.";color = darkgreen;];
    spark_5_t [label = "Rasa.r5(Resonance)";comment = "Propagation: The Vibrational Sympathy Between Beings. The empathic connection and emotional contagion—the capacity to feel what others feel, the harmonic alignment of emotional states, and the transmission of feeling across consciousness.";color = darkgreen;];
    spark_3_t [label = "Rasa.r3(Hope)";comment = "Thesis: The Positive Expectation of Future Good. The optimistic anticipation—the forward-looking emotional state that sees possibilities, believes in positive outcomes, and maintains faith despite current challenges or limitations.";color = darkblue;];
    spark_6_t [label = "Rasa.r6(Despair)";comment = "Antithesis: The Profound Sense of Hopelessness. The crushing weight of perceived impossibility—the emotional state of complete discouragement, loss of faith, and the feeling that no positive outcome is possible.";color = darkblue;];
    spark_9_t [label = "Rasa.r9(Love)";comment = "Synthesis: The Unconditional Acceptance and Connection. The transcendent emotional state—the capacity to see and accept reality as it is, to connect deeply with others and existence itself, beyond conditions and limitations.";color = darkblue;];
    spark_a_t [label = "Rasa.receive(Desire)";comment = "Potential: A raw, undirected drive. The fundamental energy of attraction and aversion—the basic movement of consciousness toward pleasure and away from pain, before it becomes refined into specific emotions or intentions.";shape = invtriangle;color = darkred;];
    spark_b_t [label = "Rasa.send(Emotion)";comment = "Manifest: The complex feeling that arises from evaluated desire. The refined emotional state—the specific feeling tone that emerges when raw desire encounters circumstances, memory, and meaning-making processes.";shape = triangle;color = darkred;];
    spark_c_t [label = "Rasa.dispatch(Expression)";comment = "Why-Who: The Authentic Communication of Feeling. The genuine sharing of emotional truth—the specific individuals, artistic forms, and communicative acts that give voice to inner experience and determine why emotions are expressed and who expresses them.";shape = doublecircle;color = darkred;];
    spark_d_t [label = "Rasa.commit(Devotion)";comment = "What-How: The Sacred Dedication to Emotional Truth. The wholehearted surrender to feeling and purpose—the specific practices, rituals, and commitments that channel emotional energy toward spiritual growth and authentic living.";shape = doublecircle;color = darkgreen;];
    spark_e_t [label = "Rasa.serve(Compassion)";comment = "When-Where: The Appropriate Application of Empathic Understanding. The wise and timely expression of care—the contextual sensitivity that knows when and where to offer emotional support, understanding, and loving presence to others.";shape = doublecircle;color = darkblue;];
    spark_f_t [label = "Rasa.exec(Pathos)";comment = "Which-Closure: The Complete Emotional Experience and Understanding. The full spectrum of feeling integrated with wisdom—the capacity to experience emotions fully while maintaining perspective, representing the mastery of emotional intelligence and aesthetic sensibility.";shape = doublecircle;color = lightgray;];
    
    spark_a_t -> spark_0_t [label = "IN"; comment = "Raw Desire flows into Essential Experience (Rasa)"; color = darkred; constraint = false;];
    spark_0_t -> spark_b_t [label = "OUT"; comment = "Essential Experience manifests as refined Emotion"; color = darkred;];
    spark_0_t -> spark_3_t [label = "REC"; comment = "Rasa generates optimistic Hope"; color = darkblue; dir = both;];
    spark_0_t -> spark_6_t [label = "REC"; comment = "Rasa embraces transformative Despair"; color = darkblue; dir = both;];
    spark_0_t -> spark_9_t [label = "REC"; comment = "Rasa culminates in unconditional Love"; color = darkblue; dir = both;];
    spark_0_t -> spark_1_t [label = "REC"; comment = "Rasa channels through conscious Will"; color = darkgreen; dir = both;];
    spark_0_t -> spark_2_t [label = "REC"; comment = "Rasa emerges from primal Instinct"; color = darkgreen; dir = both;];
    spark_0_t -> spark_4_t [label = "REC"; comment = "Rasa flows with natural Drift"; color = darkgreen; dir = both;];
    spark_0_t -> spark_8_t [label = "REC"; comment = "Rasa knows through deep Intuition"; color = darkgreen; dir = both;];
    spark_0_t -> spark_7_t [label = "REC"; comment = "Rasa expresses through authentic Expression"; color = darkgreen; dir = both;];
    spark_0_t -> spark_5_t [label = "REC"; comment = "Rasa connects through empathic Resonance"; color = darkgreen; dir = both;];
    
    spark_a_t -> spark_c_t [label = "REC"; comment = "Raw Desire seeks Expression to find meaning"; color = darkred; dir = both;];
    spark_b_t -> spark_c_t [label = "REC"; comment = "Refined Emotion emerges through Expression"; color = darkred; dir = both;];
    spark_1_t -> spark_d_t [label = "REC"; comment = "Will determines what Devotion to undertake"; color = darkgreen; dir = both;];
    spark_2_t -> spark_d_t [label = "REC"; comment = "Instinct guides how Devotion feels"; color = darkgreen; dir = both;];
    spark_4_t -> spark_d_t [label = "REC"; comment = "Drift enables Devotion surrender"; color = darkgreen; dir = both;];
    spark_8_t -> spark_d_t [label = "REC"; comment = "Intuition reveals Devotion purpose"; color = darkgreen; dir = both;];
    spark_7_t -> spark_d_t [label = "REC"; comment = "Expression manifests Devotion outwardly"; color = darkgreen; dir = both;];
    spark_5_t -> spark_d_t [label = "REC"; comment = "Resonance connects Devotion to others"; color = darkgreen; dir = both;];
    spark_3_t -> spark_e_t [label = "REC"; comment = "Hope inspires Compassionate action"; color = darkblue; dir = both;];
    spark_6_t -> spark_e_t [label = "REC"; comment = "Despair deepens Compassionate understanding"; color = darkblue; dir = both;];
    spark_9_t -> spark_e_t [label = "REC"; comment = "Love motivates Compassionate service"; color = darkblue; dir = both;];
    
    spark_1_t -> spark_2_t [label = "REC"; comment = "Will directs and refines Instinct"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_2_t -> spark_4_t [label = "REC"; comment = "Instinct surrenders to natural Drift"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_4_t -> spark_8_t [label = "REC"; comment = "Drift opens space for Intuition"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_8_t -> spark_7_t [label = "REC"; comment = "Intuition seeks Expression"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_7_t -> spark_5_t [label = "REC"; comment = "Expression creates Resonance"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_5_t -> spark_1_t [label = "REC"; comment = "Resonance strengthens collective Will"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_3_t -> spark_6_t [label = "REC"; comment = "Hope confronts the reality of Despair"; color = darkblue; dir = both; style = dashed; constraint = false;];
    spark_6_t -> spark_9_t [label = "REC"; comment = "Despair transforms into Love through acceptance"; color = darkblue; dir = both; style = dashed; constraint = false;];
    spark_9_t -> spark_3_t [label = "REC"; comment = "Love generates authentic Hope"; color = darkblue; dir = both; style = dashed; constraint = false;];
    spark_a_t -> spark_b_t [label = "REC"; comment = "Desire matures into Emotion through experience"; color = darkred; dir = both; style = dashed; constraint = false;];
    
    spark_c_t -> spark_f_t [label = "OUT"; comment = "Expression processes culminate in Pathos"; color = darkred; dir = both;];
    spark_d_t -> spark_f_t [label = "OUT"; comment = "Devotion practices embody Pathos depth"; color = darkgreen; dir = both;];
    spark_e_t -> spark_f_t [label = "OUT"; comment = "Compassion service expresses Pathos wisdom"; color = darkblue; dir = both;];
}