    spark_4_spectral_model_t [label = "Spectral.r4(Energy)";comment = "Integration: Capacity for Work and Transformation. The conserved quantity governing all physical processes—from kinetic and potential forms to the mass-energy equivalence that fuels stellar nucleosynthesis and powers cosmic evolution.";color = darkgreen;];
