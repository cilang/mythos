    spark_b_t -> spark_c_t [label = "REC"; comment = "Convergence results from effective Catalysis"; color = darkred; dir = both;];
    spark_1_t -> spark_d_t [label = "REC"; comment = "Availability enables Oath commitments"; color = darkgreen; dir = both;];
    spark_2_t -> spark_d_t [label = "REC"; comment = "Chaos challenges Oath steadfastness"; color = darkgreen; dir = both;];
    spark_4_t -> spark_d_t [label = "REC"; comment = "Destruction clears space for new Oaths"; color = darkgreen; dir = both;];
    spark_8_t -> spark_d_t [label = "REC"; comment = "Capacity determines Oath scope"; color = darkgreen; dir = both;];
    spark_7_t -> spark_d_t [label = "REC"; comment = "Order structures Oath frameworks"; color = darkgreen; dir = both;];
    spark_5_t -> spark_d_t [label = "REC"; comment = "Creation manifests Oath outcomes"; color = darkgreen; dir = both;];
    spark_3_t -> spark_e_t [label = "REC"; comment = "Luck creates Providence opportunities"; color = darkblue; dir = both;];
    spark_6_t -> spark_e_t [label = "REC"; comment = "Faith trusts in Providence guidance"; color = darkblue; dir = both;];
    spark_9_t -> spark_e_t [label = "REC"; comment = "Destiny reveals Providence patterns"; color = darkblue; dir = both;];
    
    spark_1_t -> spark_2_t [label = "REC"; comment = "Availability allows Chaos to manifest"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_2_t -> spark_4_t [label = "REC"; comment = "Chaos enables creative Destruction"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_4_t -> spark_8_t [label = "REC"; comment = "Destruction reveals hidden Capacity"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_8_t -> spark_7_t [label = "REC"; comment = "Capacity enables new Order formation"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_7_t -> spark_5_t [label = "REC"; comment = "Order provides structure for Creation"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_5_t -> spark_1_t [label = "REC"; comment = "Creation generates new Availability"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_3_t -> spark_6_t [label = "REC"; comment = "Luck requires Faith to be recognized"; color = darkblue; dir = both; style = dashed; constraint = false;];
    spark_6_t -> spark_9_t [label = "REC"; comment = "Faith reveals Destiny patterns"; color = darkblue; dir = both; style = dashed; constraint = false;];
    spark_9_t -> spark_3_t [label = "REC"; comment = "Destiny incorporates Luck elements"; color = darkblue; dir = both; style = dashed; constraint = false;];
    spark_a_t -> spark_b_t [label = "REC"; comment = "Divergence naturally converges through process"; color = darkred; dir = both; style = dashed; constraint = false;];
    
    spark_c_t -> spark_f_t [label = "OUT"; comment = "Catalysis processes define Variability range"; color = darkred; dir = both;];
    spark_d_t -> spark_f_t [label = "OUT"; comment = "Oath commitments establish Variability limits"; color = darkgreen; dir = both;];
    spark_e_t -> spark_f_t [label = "OUT"; comment = "Providence guidance shapes Variability outcomes"; color = darkblue; dir = both;];
