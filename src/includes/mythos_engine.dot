    spark_a_mythos_engine_t -> spark_c_mythos_engine_t [label = "REC"; comment = "Potential is shaped by conscious Will into purposeful action"; color = darkred; dir = both;];
    spark_b_mythos_engine_t -> spark_c_mythos_engine_t [label = "REC"; comment = "Consequences inform the evolution of cosmic Will"; color = darkred; dir = both;];
    spark_a_mythos_engine_t -> spark_b_mythos_engine_t [label = "REC"; comment = "The great cycle: Maybe becomes Consequence through conscious Choice"; color = darkred; dir = both; style = dashed; constraint = false;];
    spark_1_mythos_engine_t -> spark_d_mythos_engine_t [label = "REC"; comment = "Truth's verifications become part of the Law of Relationship"; color = darkgreen; dir = both;];
    spark_2_mythos_engine_t -> spark_d_mythos_engine_t [label = "REC"; comment = "Convergence's harmonies structure the sacred Contract"; color = darkgreen; dir = both;];
    spark_4_mythos_engine_t -> spark_d_mythos_engine_t [label = "REC"; comment = "Fixation's rhythms give permanence to Law"; color = darkgreen; dir = both;];
    spark_8_mythos_engine_t -> spark_d_mythos_engine_t [label = "REC"; comment = "Wisdom's counsel guides the evolution of Authority"; color = darkgreen; dir = both;];
    spark_7_mythos_engine_t -> spark_d_mythos_engine_t [label = "REC"; comment = "Divergence ensures Law remains flexible and just"; color = darkgreen; dir = both;];
    spark_5_mythos_engine_t -> spark_d_mythos_engine_t [label = "REC"; comment = "Excitation's vitality energizes the application of Law"; color = darkgreen; dir = both;];
    spark_3_mythos_engine_t -> spark_e_mythos_engine_t [label = "REC"; comment = "Potential finds its expression through the Vessel of Experience"; color = darkblue; dir = both;];
    spark_6_mythos_engine_t -> spark_e_mythos_engine_t [label = "REC"; comment = "Memory provides the patterns for new manifestations"; color = darkblue; dir = both;];
    spark_9_mythos_engine_t -> spark_e_mythos_engine_t [label = "REC"; comment = "Recursive becoming constantly renews the vessels of existence"; color = darkblue; dir = both;];
    
    spark_c_mythos_engine_t -> spark_f_mythos_engine_t [label = "OUT"; comment = "From the Directive flows the unseen command, the pulse of intent that sets the dance in motion; and in the Origin it finds its mirror, weaving the illusion of form and fate—Maya shimmering as if real, Fana dissolving as if it never was."; color = darkred; dir = both;];
    spark_d_mythos_engine_t -> spark_f_mythos_engine_t [label = "OUT"; comment = "Through Corpora arises the clothed word, the body of meaning incarnated in semblance; and in the Origin it unfurls as world and matter, patterns dressed in time, only to scatter back into the silence from which they came."; color = darkgreen; dir = both;];
