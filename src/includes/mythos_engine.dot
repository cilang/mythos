    spark_0_mythos_engine_t -> spark_7_mythos_engine_t [label = "REC"; comment = "The Conductor commands: Let Divergence break what must be broken!"; color = darkgreen; dir = both;];
    spark_0_mythos_engine_t -> spark_5_mythos_engine_t [label = "REC"; comment = "The Conductor commands: Let Excitation bring it to life!"; color = darkgreen; dir = both;];
    
    spark_1_mythos_engine_t -> spark_2_mythos_engine_t [label = "REC"; comment = "After Truth reveals what IS, Convergence weaves it into harmony"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_2_mythos_engine_t -> spark_4_mythos_engine_t [label = "REC"; comment = "After Convergence creates unity, Fixation gives it enduring rhythm"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_4_mythos_engine_t -> spark_8_mythos_engine_t [label = "REC"; comment = "After Fixation establishes rhythm, Wisdom finds its deeper meaning"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_8_mythos_engine_t -> spark_7_mythos_engine_t [label = "REC"; comment = "After Wisdom reveals context, Divergence liberates new possibilities"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_7_mythos_engine_t -> spark_5_mythos_engine_t [label = "REC"; comment = "After Divergence creates space, Excitation fills it with vibrant life"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_5_mythos_engine_t -> spark_1_mythos_engine_t [label = "REC"; comment = "After Excitation animates creation, Truth verifies its new reality"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_3_mythos_engine_t -> spark_6_mythos_engine_t [label = "REC"; comment = "Potential finds its purpose in the wisdom of memory"; color = darkblue; dir = both; style = dashed; constraint = false;];
    spark_6_mythos_engine_t -> spark_9_mythos_engine_t [label = "REC"; comment = "Memory provides the patterns for new becoming"; color = darkblue; dir = both; style = dashed; constraint = false;];
