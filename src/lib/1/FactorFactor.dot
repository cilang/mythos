strict digraph FactorFactor {
    label = "Factor Factor: The Principle of Factorization";
    comment = "Defines the core process of recursive decomposition and instantiation in the ACU Protocol (Factorization Protocol).";
    
    a [label = "Factor.receive(Unity)";comment = "Potential: Receives the original, unified problem space (Q=19^n).";shape = invtriangle;color = darkred;];
    0 [label = "Factor.meta(Locus)";comment = "Abstract: The specific address (w, Q, {a, 1, 3}) where the Factor is valid.";shape = doublecircle;color = darkgray;];
    b [label = "Factor.send(SubFactor)";comment = "Manifest: Outputs the resulting self-similar Sub-Factor (Q=19^n-1).";shape = triangle;color = darkred;];
    1 [label = "Factor.r1(Invariant)";comment = "Initiation: Checking for Topological Invariant preservation (Jones/Alexander).";color = darkgreen;];
    2 [label = "Factor.r2(Boundary)";comment = "Response: Redefining the Ariadi Boundary for the new Factor.";color = darkgreen;];
    4 [label = "Factor.r4(Primitives)";comment = "Integration: Inheriting and integrating necessary Cilang Primitives.";color = darkgreen;];
    8 [label = "Factor.r8(Scaling)";comment = "Reflection: Performing the Quantization Scaling (Q=19^n to Q=19^n-1).";color = darkgreen;];
    7 [label = "Factor.r7(Topology)";comment = "Consolidation: Consolidating the Knot Topology of the resulting Sub-Factor.";color = darkgreen;];
    5 [label = "Factor.r5(Signature)";comment = "Propagation: Propagating the new Factor Signature (relational graph).";color = darkgreen;];
    3 [label = "Factor.r3(Decomposition)";comment = "Thesis: The initial step of decomposition (breaking T(19,2) into simpler braids).";color = darkblue;];
    6 [label = "Factor.r6(Composition)";comment = "Antithesis: The final step of composition (recombining the solutions via the T(19,2) join).";color = darkblue;];
    9 [label = "Factor.r9(Equivalence)";comment = "Synthesis: Ensuring the initial problem is equivalent to the sum of its factored parts.";color = darkblue;];
    c [label = "Factor.dispatch(Split)";comment = "Interface: The Split Operation that formally divides the factor space.";shape = doublecircle;color = darkred;];
    d [label = "Factor.commit(Graph)";comment = "Process: The committed Relational Graph that defines the Factor's structure.";shape = doublecircle;color = darkgreen;];
    e [label = "Factor.serve(Closure)";comment = "Logic: The Factor Closure Rule—ensuring all loops are closed and links satisfied.";shape = doublecircle;color = darkblue;];
    f [label = "Factor.exec(Recursion)";comment = "Closure: The validated process of Recursion—self-similarity and re-initiation confirmed.";shape = doublecircle;color = lightgray;];
}