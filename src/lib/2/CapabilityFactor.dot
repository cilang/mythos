strict digraph CapabilityFactor {
    label = "Capability";
    comment = "The divine gatekeeper of potential—where petitions become permissions and possibilities transform into empowered realities. The sacred architecture of 'what may be' in the cosmos.";
    
    a [label = "Capability.receive(Petition)";comment = "The Humble Request. A soul's earnest appeal for the authority to act, create, or transform—the sacred act of seeking empowerment from the cosmic order.";shape = invtriangle;color = darkred;];
    0 [label = "Capability.meta(Capability)";comment = "The Wellspring of Potential. The primordial source of all authority and capacity in existence—not as control, but as the fundamental right of conscious beings to participate in creation.";shape = doublecircle;color = darkgray;];
    b [label = "Capability.send(Manifest)";comment = "The Empowered Action. The granted authority made manifest in the world—the moment when potential becomes actual power to shape reality according to cosmic law.";shape = triangle;color = darkred;];
    1 [label = "Capability.r1(Request)";comment = "The Sacred Appeal. The formalized expression of need or desire for specific capacities—the first step in the dance between mortal will and cosmic permission.";color = darkgreen;];
    2 [label = "Capability.r2(Assess)";comment = "The Weighing of Worth. The careful evaluation of a petition against cosmic harmony, karmic readiness, and the greater good—the wisdom that prevents power from falling into unprepared hands.";color = darkgreen;];
    4 [label = "Capability.r4(Grant)";comment = "The Divine Yes. The formal bestowal of specific capacities and authorities—the sacred moment when the cosmos says 'you may' and a being's potential expands.";color = darkgreen;];
    8 [label = "Capability.r8(Audit)";comment = "The Eternal Witness. The continuous monitoring of how granted capabilities are used—the cosmic accountability that ensures power serves life, not domination.";color = darkgreen;];
    7 [label = "Capability.r7(Revoke)";comment = "The Necessary No. The solemn withdrawal of misused or unneeded capacities—the protective mechanism that prevents cosmic harm from abused power.";color = darkgreen;];
    5 [label = "Capability.r5(Propagate)";comment = "The Ripple of Empowerment. The lawful delegation and sharing of capabilities with others—how wisdom and authority spread through the cosmic network of beings.";color = darkgreen;];
    3 [label = "Capability.r3(Permission)";comment = "Thesis: The Cosmic Allowance. The fundamental right to exist, act, and participate in creation—the baseline authorization that underlies all specific capabilities.";color = darkblue;];
    6 [label = "Capability.r6(Entitlement)";comment = "Antithesis: The Earned Right. Specific authorities gained through mastery, service, or evolution—the capacities that must be won through effort and integrity.";color = darkblue;];
    9 [label = "Capability.r9(Potential)";comment = "Synthesis: The Latent Power. The union of inherent permission and earned entitlement—the total capacity of a being to grow, create, and transform reality.";color = darkblue;];
    c [label = "Capability.dispatch(Will)";comment = "The Focused Intention. The channeled application of granted capabilities toward specific purposes—where cosmic permission meets conscious direction in the act of creation.";shape = doublecircle;color = darkred;];
    d [label = "Capability.commit(Charter)";comment = "The Sacred Contract. The immutable record of all granted capabilities, their terms, and their cosmic purposes—the constitutional framework of empowered existence.";shape = doublecircle;color = darkgreen;];
    e [label = "Capability.serve(Domain)";comment = "The Sphere of Influence. The specific realm or aspect of reality where granted capabilities apply—the bounded space where authority finds its proper expression.";shape = doublecircle;color = darkblue;];
    f [label = "Capability.exec(LawOfCapability)";comment = "The Meta-Principle of Power. The highest law governing all capabilities: 'All Power Must Serve Growth; No Authority May Create Eternal Servitude.' This ensures empowerment never becomes oppression.";shape = doublecircle;color = lightgray;];
}