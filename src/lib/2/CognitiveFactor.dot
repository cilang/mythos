    {1 2 4 8 7 5} -> d [dir = both; color = "red";];
    {3 6 9} -> e [dir = both; color = "red";];
    {c d e} -> f [dir = both; color = "red";];
    
    a -> b -> a [dir = both; color = "blue";];
