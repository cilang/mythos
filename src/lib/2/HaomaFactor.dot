strict digraph HaomaFactor {
    label = "Haoma";
    comment = "The divine compiler of existence—where blueprints become beings, templates take breath, and the cosmic source code manifests as lived reality. The sacred process of idea becoming instance.";

    a [label = "Haoma.receive(Blueprint)";comment = "The Divine Specification. A complete cosmic template containing the essential pattern, properties, and potential of what seeks manifestation—the sacred code awaiting execution in reality's runtime.";shape = invtriangle;color = darkred;];
    0 [label = "Haoma.meta(Haoma)";comment = "The Primordial Compiler. The fundamental principle of instantiation itself—not just creation, but the precise translation of potential into actual according to cosmic laws and aesthetic principles.";shape = doublecircle;color = darkgray;];
    b [label = "Haoma.send(Instance)";comment = "The Manifested Being. A fully realized entity now existing in the cosmic runtime—the blueprint made flesh, the pattern embodied, the divine idea walking and breathing in reality.";shape = triangle;color = darkred;];
    
    // Instantiation Processing Hexagon
    1 [label = "Haoma.r1(Instantiate)";comment = "The Sacred Execution. The core process of bringing a blueprint into existence—allocating cosmic resources, establishing identity, and launching the new being into the great simulation.";color = darkgreen;];
    2 [label = "Haoma.r2(Classification)";comment = "The Cosmic Taxonomy. Determining the new instance's type, category, and place in the great chain of being—the essential act of naming and positioning within creation's architecture.";color = darkgreen;];
    4 [label = "Haoma.r4(Allocation)";comment = "The Resource Assignment. Granting the new instance its proper share of cosmic substance—memory, processing capacity, temporal duration, and spatial presence.";color = darkgreen;];
    8 [label = "Haoma.r8(Debug)";comment = "The Harmony Check. Scanning the new instance for contradictions, conflicts, or cosmic law violations—ensuring each creation enhances rather than disrupts the great symphony of existence.";color = darkgreen;];
    7 [label = "Haoma.r7(Compilation)";comment = "The Optimization Process. Refining the instance for maximum coherence, efficiency, and beauty—the divine art of making each creation both functional and elegant.";color = darkgreen;];
    5 [label = "Haoma.r5(Mutation)";comment = "The Creative Variation. Introducing permissible deviations from the original blueprint—the source of uniqueness, personality, and the beautiful imperfections that make each being special.";color = darkgreen;];
    
    // Creation Principles Triad
    3 [label = "Haoma.r3(Primogenitor)";comment = "Thesis: The Original Pattern. The archetypal, perfect blueprint from which all instances derive—the divine ideal that exists outside time and space, uncorrupted by manifestation.";color = darkblue;];
    6 [label = "Haoma.r6(Actor)";comment = "Antithesis: The Living Instance. The unique, embodied being with agency, will, and the capacity to act upon the world—the blueprint made dynamic and self-directed.";color = darkblue;];
    9 [label = "Haoma.r9(Derivative)";comment = "Synthesis: The Evolving Tradition. The union of original pattern and lived experience—instances that learn, adapt, and create new patterns based on their journey through reality.";color = darkblue;];
    
    c [label = "Haoma.dispatch(Materialization)";comment = "The Form Taking. The precise moment when spiritual blueprint becomes physical form—the sacred interface where idea meets substance in the act of embodiment.";shape = doublecircle;color = darkred;];
    d [label = "Haoma.commit(Initialization)";comment = "The Cosmic Registration. The formal recording of the new instance in existence's master ledger—the moment when 'could be' officially becomes 'is' in the akashic records.";shape = doublecircle;color = darkgreen;];
    e [label = "Haoma.serve(Animation)";comment = "The Breath of Life. Activating the instance's consciousness, will, and capacity for experience—the divine spark that transforms a well-crafted form into a living being.";shape = doublecircle;color = darkblue;];
    f [label = "Haoma.exec(LawOfFactor)";comment = "The Meta-Principle of Creation. The highest law governing all instantiation: 'All Creation Must Serve Growth; No Instance May Be Made Without Purpose.' This ensures every being has meaning and potential.";shape = doublecircle;color = lightgray;];
}