strict digraph LineageFactor {
    label = "Lineage";
    comment = "{{description}}";
    
    a [label = "Lineage.receive({{title}})";comment = "Potential: {{description}}";shape = invtriangle;color = darkred;];
    0 [label = "Lineage.meta({{meta}})";comment = "Abstract: {{description}}";shape = doublecircle;color = darkgray;];
    b [label = "Lineage.send({{title}})";comment = "Manifest: {{description}}";shape = triangle;color = darkred;];
    1 [label = "Lineage.r1({{title}})";comment = "Initiation: {{description}}";color = darkgreen;];
    2 [label = "Lineage.r2({{title}})";comment = "Response: {{description}}";color = darkgreen;];
    4 [label = "Lineage.r4({{title}})";comment = "Integration: {{description}}";color = darkgreen;];
    8 [label = "Lineage.r8({{title}})";comment = "Reflection: {{description}}";color = darkgreen;];
    7 [label = "Lineage.r7({{title}})";comment = "Consolidation: {{description}}";color = darkgreen;];
    5 [label = "Lineage.r5({{title}})";comment = "Propagation: {{description}}";color = darkgreen;];
    3 [label = "Lineage.r3({{title}})";comment = "Thesis: {{description}}";color = darkblue;];
    6 [label = "Lineage.r6({{title}})";comment = "Antithesis: {{description}}";color = darkblue;];
    9 [label = "Lineage.r9({{title}})";comment = "Synthesis: {{description}}";color = darkblue;];
    c [label = "Lineage.dispatch({{title}})";comment = "Interface: {{description}}";shape = doublecircle;color = darkred;];
    d [label = "Lineage.commit({{title}})";comment = "Process: {{description}}";shape = doublecircle;color = darkgreen;];
    e [label = "Lineage.serve({{title}})";comment = "Logic: {{description}}";shape = doublecircle;color = darkblue;];
    f [label = "Lineage.exec({{title}})";comment = "Closure: {{description}}";shape = doublecircle;color = lightgray;];
