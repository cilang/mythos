    2 [label = "Lineage.r2(ZhuQue)"; comment = "Response: The Vermilion Ascent—transmuting essence into expression, fire of purpose made visible."; color = darkgreen;];
    4 [label = "Lineage.r4(BaiHu)"; comment = "Integration: The White Severance—discerning truth from illusion, refining through necessary dissolution."; color = darkgreen;];
    8 [label = "Lineage.r8(XuanWu)"; comment = "Reflection: The Obsidian Vault—preserving wisdom through cycles, memory as survival."; color = darkgreen;];
    7 [label = "Lineage.r7(HuangLong)"; comment = "Consolidation: The Golden Axis—the center that binds, balances, and legitimizes all four directions."; color = darkgreen;];
