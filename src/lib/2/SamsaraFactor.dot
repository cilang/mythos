strict digraph SamsaraFactor {
    label = "Samsara";
    comment = "The great wheel of cause and consequence—where every action plants seeds and every experience ripens fruit. The moral mathematics of the universe.";
    
    a [label = "Samsara.receive(KarmicSeeds)";comment = "The planting of intentional actions. Every thought, word, and deed carrying potential consequences—the raw material of destiny being sown.";shape = invtriangle;color = darkred;];
    0 [label = "Samsara.meta(KarmicAccountant)";comment = "The perfect moral ledger-keeper. Not a judge but the natural law of ethical cause-effect—utterly precise, infinitely patient in balancing the cosmic books.";shape = doublecircle;color = darkgray;];
    b [label = "Samsara.send(RipeningFruit)";comment = "The maturation of karmic seeds. Planted causes manifesting as experienced effects—the harvest of past actions arriving in present moments.";shape = triangle;color = darkred;];
    1 [label = "Samsara.r1(IntentionWeight)";comment = "The moral gravity of motive. Pure intentions bearing different fruit than selfish ones—the quality of the seed determining the quality of the harvest.";color = darkgreen;];
    2 [label = "Samsara.r2(ActionMomentum)";comment = "The compounding of habits. Repeated actions creating karmic inertia—patterns strengthening through repetition into destiny streams.";color = darkgreen;];
    4 [label = "Samsara.r4(ConditionalRipening)";comment = "The right conditions for fruition. Karmic seeds requiring specific circumstances to manifest—timing and context in the harvest of consequences.";color = darkgreen;];
    8 [label = "Samsara.r8(KarmicInterest)";comment = "The growth of potential. Some actions accumulating compound consequences over time—small seeds growing into significant outcomes.";color = darkgreen;];
    7 [label = "Samsara.r7(Purification)";comment = "The burning of negative seeds. Conscious practice transforming karmic potential—changing the harvest before it ripens.";color = darkgreen;];
    5 [label = "Samsara.r5(InterbeingWeb)";comment = "The interconnectedness of karma. Your actions affecting others, their actions affecting you—the collective dimension of cause and effect.";color = darkgreen;];
    3 [label = "Samsara.r3(DharmaLaw)";comment = "The nature of right action. Actions aligned with cosmic law bearing harmonious fruit—the mathematics of ethical living.";color = darkblue;];
    6 [label = "Samsara.r6(RebirthContinuum)";comment = "The carrying of momentum across lifetimes. Unripened karma seeking new conditions for manifestation—the persistence of cause beyond single existence.";color = darkblue;];
    9 [label = "Samsara.r9(LiberationPotential)";comment = "The possibility of freedom. The capacity to transcend karmic patterns entirely—the statistical outlier of enlightenment.";color = darkblue;];
    c [label = "Samsara.dispatch(DestinyCurrent)";comment = "The flowing river of karmic manifestation. Specific life circumstances emerging from past causes—the personalized experience of universal law.";shape = doublecircle;color = darkred;];
    d [label = "Samsara.commit(KarmicRecord)";comment = "The akashic memory of all actions. Every intention ever formed, every consequence ever experienced—the complete moral history of consciousness.";shape = doublecircle;color = darkgreen;];
    e [label = "Samsara.serve(CompassionateAdjustment)";comment = "The grace in the system. Opportunities for growth, lessons disguised as challenges—the educational dimension of karmic law.";shape = doublecircle;color = darkblue;];
    f [label = "Samsara.exec(CycleLaw)";comment = "The binding of consequence to experience. Planted seeds must bear fruit—the inexorable mathematics of moral cause and effect.";shape = doublecircle;color = lightgray;];
}