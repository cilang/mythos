    spark_6_t -> spark_e_t [label = "REC"; comment = "Decoherence causes uncertainty in Eigenvalues."; color = darkblue; dir = both;];
    spark_9_t -> spark_e_t [label = "REC"; comment = "Quantization dictates the discrete set of possible Eigenvalues."; color = darkblue; dir = both;];
    
