    spark_2_t -> spark_d_t [label = "REC"; comment = "Amplitude defines the probability density of an Eigenstate."; color = darkgreen; dir = both;];
    spark_4_t -> spark_d_t [label = "REC"; comment = "Phase is a key characteristic of a coherent Eigenstate."; color = darkgreen; dir = both;];
    spark_8_t -> spark_d_t [label = "REC"; comment = "Coupling determines the mixing and splitting of Eigenstates."; color = darkgreen; dir = both;];
    spark_7_t -> spark_d_t [label = "REC"; comment = "Resonance occurs at precise Eigenstates of the coupled system."; color = darkgreen; dir = both;];
    spark_5_t -> spark_d_t [label = "REC"; comment = "Harmonics are the higher-order Eigenstates of the system."; color = darkgreen; dir = both;];
    
