    spark_0_t -> spark_7_t [label = "REC"; comment = "A spectral identity achieves stability through resonance."; color = darkgreen; dir = both;];
    spark_0_t -> spark_5_t [label = "REC"; comment = "A spectral identity extends into harmonic structures."; color = darkgreen; dir = both;];
    
