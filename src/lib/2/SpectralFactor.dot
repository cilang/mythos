    spark_6_t -> spark_9_t [label = "REC"; comment = "Decoherence is the process that reveals the underlying Quantization."; color = darkblue; dir = both; style = dashed; constraint = false;];
    spark_9_t -> spark_3_t [label = "REC"; comment = "Quantization enables the existence of long-lived, coherent states."; color = darkblue; dir = both; style = dashed; constraint = false;];
    
