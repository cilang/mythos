    spark_b_t -> spark_c_t [label = "REC"; comment = "Interference patterns reveal the structure of the Operator."; color = darkred; dir = both;];
    spark_a_t -> spark_b_t [label = "REC"; comment = "The fundamental dance between raw vibration and structured pattern."; color = darkred; dir = both; style = dashed; constraint = false;];
    
