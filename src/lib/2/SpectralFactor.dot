    spark_0_t -> spark_6_t [label = "REC"; comment = "Spectral identity is subject to Decoherence from its environment."; color = darkblue; dir = both;];
    spark_0_t -> spark_9_t [label = "REC"; comment = "Spectral identity is fundamentally Quantized."; color = darkblue; dir = both;];
    
