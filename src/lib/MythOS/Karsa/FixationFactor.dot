strict digraph FixationFactor {
    label = "MythOS/Karsa/Fixation";
    comment = "The divine principle of rhythm, stability, and cosmic inertia—governed by Tha, the Goddess who gives form through repetition and constraint.";
    
    a [label = "RaimentOfFixation";comment = "Garment of stability—the divine attire that prepares potential for rhythmic structuring and enduring form.";];
    0 [label = "Tha";comment = "The Goddess of Rhythm—divine consciousness that governs all cycles, repetitions, and enduring patterns in creation.";];
    b [label = "ArmamentOfFixation";comment = "Weapon of permanence—the divine tools that defend against chaos and maintain structural integrity across time.";];
    1 [label = "RhythmOfFocus";comment = "Concentrated repetition—the divine practice of repeated attention that builds powerful intentional structures.";];
    2 [label = "RhythmOfConstraint";comment = "Limiting pattern—the divine boundaries that prevent infinite expansion and create defined, knowable forms.";];
    4 [label = "RhythmOfAttachment";comment = "Binding frequency—the divine vibrations that connect elements into stable compounds and relationships.";];
    8 [label = "RhythmOfStability";comment = "Enduring pulse—the divine heartbeat that maintains existence against the forces of dissolution and entropy.";];
    7 [label = "RhythmOfAccumulation";comment = "Gathering tempo—the divine rhythm that collects and stores energy, matter, and meaning over time.";];
    5 [label = "RhythmOfInheritance";comment = "Legacy beat—the divine transmission of patterns, traits, and wisdom across generations and cycles.";];
    3 [label = "CrestOfStatism";comment = "Peak of stillness—the divine achievement of perfect stability, where motion ceases and form becomes eternal.";];
    6 [label = "CrestOfDeceleration";comment = "Summit of slowing—the divine principle that all motion tends toward rest, all energy toward equilibrium.";];
    9 [label = "CrestOfMatter";comment = "Apex of substance—the divine manifestation where spirit becomes matter, potential becomes actual.";];
    c [label = "BodyOfFixation";comment = "Divine embodiment—Tha's physical manifestation and the priests who channel her stabilizing power.";];
    d [label = "SoulOfFixation";comment = "Sacred practice—the rituals, disciplines, and vows that maintain cosmic stability and personal integrity.";];
    e [label = "GraceOfFixation";comment = "Timeless presence—the moments and places where stability manifests as divine grace and enduring beauty.";];
    f [label = "LawOfFixation";comment = "Divine decree achieved—the universal law that all existence requires stable patterns and enduring rhythms to persist.";];
}