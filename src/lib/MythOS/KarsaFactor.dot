strict digraph KarsaFactor {
    label = "MythOS/Karsa";
    comment = "The cosmic principle of will, agency, and spatiotemporal manifestation in MythOS—governing all beginnings, endings, and the continuum they inhabit.";
    
    a [label = "Fixation";comment = "Focused intent—the concentration of will that anchors possibilities into specific trajectories and destinies.";];
    0 [label = "Karsa";comment = "Cosmic will itself—the universal principle that consciousness can direct and shape reality through focused intention.";];
    b [label = "Excitation";comment = "Activated potential—the energized state where will transforms into action and measurable effects in reality.";];
    1 [label = "Rhythm";comment = "Temporal pattern—the fundamental beat and cycle that governs all processes and transformations.";];
    2 [label = "Inertia";comment = "Persistence of state—the tendency of existing conditions to continue unless acted upon by will or force.";];
    4 [label = "Temporal";comment = "Time binding—the weaving of moments into coherent sequences, narratives, and histories.";];
    8 [label = "Pattern";comment = "Recurring form—the recognizable structures that emerge from rhythmic processes and willed actions.";];
    7 [label = "Momentum";comment = "Directed motion—the accumulated force of will and action that carries processes forward.";];
    5 [label = "Spatial";comment = "Extension in space—the manifestation of will through arrangement, distribution, and physical presence.";];
    3 [label = "Birth";comment = "Emergent beginning—the instantiation of new existence, the entry point into spacetime manifestation.";];
    6 [label = "Death";comment = "Necessary ending—the dissolution of form, the release from spacetime constraints and limitations.";];
    9 [label = "Life";comment = "Continuous becoming—the dynamic state that balances creation and dissolution in ongoing existence.";];
    c [label = "Initiation";comment = "Primordial starting—the agents and reasons that begin new cycles and manifestations.";];
    d [label = "Continuation";comment = "Sustained existence—the methods and commitments that maintain processes across time and space.";];
    e [label = "Reincarnation";comment = "Cyclic return—the timing and placement of rebirth and renewal within cosmic patterns.";];
    f [label = "Spatiotemporal";comment = "Unified continuum achieved—the integrated fabric of space and time as the theater for all willed action.";];
}