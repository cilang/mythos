strict digraph ObserverFactor {
    label = "MythOS/Observer";
    comment = "The cosmic consciousness principle in MythOS—addressing the ultimate questions of origin, purpose, and destination (Sangkan Paraning Dumadi).";
    
    a [label = "Sangkan";comment = "Source inquiry—the fundamental questioning of origins: 'From where do I come? What is my essential nature?'";];
    0 [label = "Dumadi";comment = "Existential ground—the universal principle of being itself, the is-ness that underlies all manifestation and observation.";];
    b [label = "Paran";comment = "Destination realization—the actualized understanding of purpose: 'Where am I going? What is my ultimate destination?'";];
    1 [label = "Resource";comment = "Existential capital—the innate capacities, talents, and energies available for the cosmic journey.";];
    2 [label = "Authority";comment = "Self-governance—the right and responsibility to direct one's own path and make existential choices.";];
    4 [label = "Relation";comment = "Cosmic connectivity—the web of relationships and responsibilities that situate the observer in the whole.";];
    8 [label = "Axiom";comment = "Foundational truth—the self-evident principles that guide the journey and cannot be further reduced.";];
    7 [label = "Knowledge";comment = "Accumulated wisdom—the understanding gained through experience, study, and contemplation of the cosmic order.";];
    5 [label = "Lineage";comment = "Ancestral stream—the transmission of wisdom, karma, and existential patterns across generations and lifetimes.";];
    3 [label = "Assumption";comment = "Unquestioned ground—the foundational beliefs and presuppositions that shape perception without conscious examination.";];
    6 [label = "Suggestion";comment = "Alternative possibility—the competing interpretations and potential realities that challenge existing assumptions.";];
    9 [label = "Perception";comment = "Clarified seeing—the achieved state of awareness where reality is perceived as it truly is, beyond assumptions and suggestions.";];
    c [label = "Why_Who";comment = "Purpose and identity—the fundamental questions of motivation and selfhood that drive the existential journey.";];
    d [label = "What_How";comment = "Method and action—the specific practices, disciplines, and choices that constitute the path of awakening.";];
    e [label = "When_Where";comment = "Context and timing—the appropriate moments and situations for different stages of the spiritual journey.";];
    f [label = "Which_Closure";comment = "Choice and completion—the final selection of path and the achievement of existential understanding (Sangkan Paran Dumadi).";];
}