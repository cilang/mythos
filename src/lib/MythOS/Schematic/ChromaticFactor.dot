strict digraph ChromaticFactor {
    label = "MythOS/Schematic/Chromatic";
    comment = "The qualitative dimension of reality. Governs the perception, emission, and interaction of qualities through the logic of hue, saturation, and value.";
    
    a [label = "Golden";comment = "The source of pure, unmodulated value and luminance—the potential for all brilliance and warmth.";];
    0 [label = "Chroma";comment = "The invariant essence of chromaticity itself—the principle of qualitative difference.";];
    b [label = "Silver";comment = "The reflected, modulated, and contextualized quality—the actual perceived color in a specific environment.";];
    1 [label = "Hue";comment = "The fundamental spectral identity—the pure wavelength or primary quality that defines a color's core nature.";];
    2 [label = "Saturation";comment = "The intensity or purity of the Hue—its vibrancy versus dilution with achromatic light.";];
    4 [label = "Value";comment = "The overall lightness or brightness—the luminous intensity of the color.";];
    8 [label = "Complement";comment = "The opposing hue that, when mixed, produces achromatic gray—the necessary contrast for definition.";];
    7 [label = "Harmony";comment = "The pleasing or stable relationship between multiple hues, based on their geometric arrangement on the color wheel.";];
    5 [label = "Gamut";comment = "The reproducible range of colors available in a given context (a device, a medium, a perception).";];
    3 [label = "Achromatic";comment = "The realm of pure value and form—zero hue, where structure is revealed without chromatic distraction.";];
    6 [label = "Polychromatic";comment = "The chaotic potential of the full spectrum—maximum hue diversity before organization.";];
    9 [label = "Monochromatic";comment = "The ordered expression of a single hue domain—coherent qualitative identity across variations in saturation and value.";];
    c [label = "Perception";comment = "The act of qualitative discernment—the biological, psychological, or instrumental process that interprets wavelengths as color.";];
    d [label = "HSB_HSV";comment = "The unified color model—the cylindrical-coordinate representation of Hue, Saturation, and Value/Brightness as the method for defining any color.";];
    e [label = "Illumination";comment = "The contextual lighting and environment that determines how a color is rendered and perceived.";];
    f [label = "Qualia";comment = "The final, subjective experience of a specific color—the 'redness of red' as a realized, ineffable quality.";];
}