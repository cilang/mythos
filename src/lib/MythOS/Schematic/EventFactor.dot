strict digraph EventFactor {
    label = "MythOS/Schematic/Event";
    comment = "The mathematics of maybe—where numbers dance with uncertainty and statistics become destiny. The cold, beautiful equations of chance that underlie all manifestation.";
    
    a [label = "ProbabilityCloud";comment = "The quantum soup of potential outcomes. Every possible timeline, every might-be and could-be, swirling in mathematical purity before selection.";];
    0 [label = "RandomOracle";comment = "The perfect random number generator of existence. Not a conscious being, but the mathematical principle of entropy itself—utterly impartial, infinitely creative in its permutations.";];
    b [label = "StatisticalManifestation";comment = "The collapsed wave function. The specific outcome selected from the probability distribution—reality choosing one path from many.";];
    1 [label = "Correlation";comment = "The dance of related probabilities. How one outcome influences another—covariance matrices of destiny weaving complex interdependencies.";];
    2 [label = "Distribution";comment = "The shape of possibility. Bell curves, power laws, Poisson distributions—the architectural forms that probability takes across different domains.";];
    4 [label = "Regression";comment = "The tendency toward mean. Extreme outcomes naturally moderating over time—the statistical gravity that pulls exceptional toward average.";];
    8 [label = "Clustering";comment = "The emergence of patterns. Random events grouping non-randomly—the universe's tendency to create order from chaos through sheer statistics.";];
    7 [label = "Convergence";comment = "The law of large numbers. How probabilities stabilize as samples grow—certainty emerging from uncertainty through repetition.";];
    5 [label = "EntropicDrift";comment = "The arrow of statistical time. Systems evolving toward more probable states—the thermodynamic gradient of possibility.";];
    3 [label = "QuantumIndeterminacy";comment = "Fundamental uncertainty. The built-in randomness at reality's foundation—probability as a primary quality of existence.";];
    6 [label = "ChaosSensitivity";comment = "The butterfly effect. Minute changes creating vastly different outcomes—probability's amplification of initial conditions.";];
    9 [label = "ComplexityEmergence";comment = "Simple rules, complex results. How deterministic probabilities generate unpredictable behaviors—order born from randomness.";];
    c [label = "OutcomeVector";comment = "The specific result array. All manifested outcomes from the probability field—the actualized events ready for cosmic processing.";];
    d [label = "StatisticalMemory";comment = "The accumulated data universe. Every outcome ever manifested, building the cosmic probability database—reality learning its own habits.";];
    e [label = "PossibilitySpace";comment = "The ongoing generation of new probabilities. Even as outcomes manifest, new possibilities emerge—infinite creativity through finite rules.";];
    f [label = "ManifestationLaw";comment = "The binding of outcome to reality. What was probable becomes actual—the final step where statistics become history.";];
}