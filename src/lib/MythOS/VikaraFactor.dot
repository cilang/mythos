strict digraph VikaraFactor {
    label = "MythOS/Vikara";
    comment = "The cosmic principle of transformation and change in MythOS—governing all processes of divergence, convergence, and metamorphosis.";
    
    a [label = "Divergence";comment = "Branching possibilities—the infinite paths reality could take, the raw material of all change and variation.";];
    0 [label = "Vikara";comment = "Transformation itself—the universal principle that all things can and must change their state, form, or nature.";];
    b [label = "Convergence";comment = "Synthesized outcomes—the coming together of disparate elements into new unified wholes and resolutions.";];
    1 [label = "Availability";comment = "Open potential—the state of readiness for change, when conditions permit transformation to begin.";];
    2 [label = "Chaos";comment = "Creative dissolution—the breakdown of existing patterns that makes space for new arrangements to emerge.";];
    4 [label = "Destruction";comment = "Necessary ending—the deliberate dismantling of old forms to liberate their constituent elements.";];
    8 [label = "Capacity";comment = "Transformative potential—the measure of how much change a system can undergo or contain.";];
    7 [label = "Order";comment = "Emergent structure—the new patterns and organizations that crystallize from transformative processes.";];
    5 [label = "Creation";comment = "Novel emergence—the birth of entirely new forms, relationships, and possibilities.";];
    3 [label = "Luck";comment = "Fortuitous chance—the aspect of transformation that operates beyond predictable cause and effect.";];
    6 [label = "Faith";comment = "Trust in process—the commitment to transformation even when outcomes cannot be guaranteed or seen.";];
    9 [label = "Destiny";comment = "Meaningful pattern—the emergent purpose that gives direction and significance to transformative journeys.";];
    c [label = "Catalysis";comment = "Change agents and triggers—the specific catalysts and actors that initiate or guide transformations.";];
    d [label = "Oath";comment = "Binding commitment to change—the promises and contracts that lock in transformative pathways.";];
    e [label = "Providence";comment = "Guided unfolding—the timing and placement of transformations within larger cosmic patterns.";];
    f [label = "Variability";comment = "Dynamic stability achieved—the state where change itself becomes the constant, reliable principle.";];
}