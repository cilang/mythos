    spark_b_t -> spark_c_t [label = "REC"; comment = "Manifestation demonstrates Authority effectiveness"; color = darkred; dir = both;];
    spark_1_t -> spark_d_t [label = "REC"; comment = "Truth becomes measurable through Observation"; color = darkgreen; dir = both;];
