    spark_0_t -> spark_6_t [label = "REC"; comment = "Structure manifests spectral Possibility (Spectral)"; color = darkblue; dir = both;];
    spark_0_t -> spark_9_t [label = "REC"; comment = "Structure integrates chromatic Harmony (Chroma)"; color = darkblue; dir = both;];
