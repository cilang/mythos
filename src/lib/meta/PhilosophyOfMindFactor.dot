    spark_9_t -> spark_3_t [label = "REC"; comment = "Panpsychism seeks reconciliation with physical science"; color = darkblue; dir = both; style = dashed; constraint = false;];
    spark_a_t -> spark_b_t [label = "REC"; comment = "Subjective ground manifests as experienced world"; color = darkred; dir = both; style = dashed; constraint = false;];
    
    spark_c_t -> spark_f_t [label = "REC"; comment = "First-person authority informs consciousness models"; color = darkred; dir = both;];
