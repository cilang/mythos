    spark_1_t -> spark_d_t [label = "REC"; comment = "Qualia problems demand explanatory frameworks"; color = darkgreen; dir = both;];
    spark_2_t -> spark_d_t [label = "REC"; comment = "Intentionality requires theoretical explanation"; color = darkgreen; dir = both;];
    spark_4_t -> spark_d_t [label = "REC"; comment = "Embodiment constrains possible explanations"; color = darkgreen; dir = both;];
    spark_8_t -> spark_d_t [label = "REC"; comment = "Phenomenology provides data for theories"; color = darkgreen; dir = both;];
    spark_7_t -> spark_d_t [label = "REC"; comment = "Free will debates shape explanatory choices"; color = darkgreen; dir = both;];
    spark_5_t -> spark_d_t [label = "REC"; comment = "Unity problems guide framework development"; color = darkgreen; dir = both;];
    spark_3_t -> spark_e_t [label = "REC"; comment = "Physicalism grounds mind in biological context"; color = darkblue; dir = both;];
    spark_6_t -> spark_e_t [label = "REC"; comment = "Dualism situates mind in cultural traditions"; color = darkblue; dir = both;];
    spark_9_t -> spark_e_t [label = "REC"; comment = "Panpsychism embeds consciousness in cosmic context"; color = darkblue; dir = both;];
    
    spark_1_t -> spark_2_t [label = "REC"; comment = "Qualitative experience naturally exhibits intentionality"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_2_t -> spark_4_t [label = "REC"; comment = "Intentionality requires embodied engagement with world"; color = darkgreen; dir = both; style = dashed; constraint = false;];
    spark_4_t -> spark_8_t [label = "REC"; comment = "Embodied cognition invites phenomenological investigation"; color = darkgreen; dir = both; style = dashed; constraint = false;];
