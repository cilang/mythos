    spark_d_meta_view_t [label = "Meta.commit(Which)";comment = "What-How: The Act of Definition. The moment of decisive selection—resolving 'Which specific answer?' among infinite possibilities, committing to a particular manifestation of truth.";shape = doublecircle;color = darkgreen;];
    spark_e_meta_view_t [label = "Meta.serve(Relationship)";comment = "When-Where: The Web of Connection. Serves the intricate network of relationships between all concepts—the living matrix where IN/OUT and the 6Ws find their eternal dance partners.";shape = doublecircle;color = darkblue;];
    spark_f_meta_view_t [label = "Meta.exec(Closure)";comment = "Which-Closure: The Completion of Pattern. The final binding of inquiry into knowing—the moment a question becomes answer, a pattern becomes reality, and the cosmic grammar speaks a new word into existence.";shape = doublecircle;color = lightgray;];
    
