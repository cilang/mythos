    spark_0_meta_view_t [label = "Meta.meta(Meta)";comment = "The Concept of Concept Itself. The primordial abstraction from which all other abstractions emerge. The silent witness that knows what knowing is, the thought that comprehends thought itself.";shape = doublecircle;color = darkgray;];
    spark_1_meta_view_t [label = "Meta.r1(What)";comment = "The First Question: Essence and Substance. The inquiry into the fundamental nature of being—what something IS in its core identity, stripped of context and circumstance.";color = darkgreen;];
    spark_2_meta_view_t [label = "Meta.r2(When)";comment = "The Second Question: Time and Sequence. The inquiry into temporal context—when events unfold, in what order, and according to what rhythm of becoming.";color = darkgreen;];
    spark_4_meta_view_t [label = "Meta.r4(How)";comment = "The Third Question: Method and Process. The inquiry into transformation—how change occurs, through what mechanisms, and by what means potential becomes actual.";color = darkgreen;];
    spark_8_meta_view_t [label = "Meta.r8(Why)";comment = "The Fourth Question: Cause and Purpose. The inquiry into meaning—why things happen, for what reason, and toward what end existence unfolds.";color = darkgreen;];
    spark_7_meta_view_t [label = "Meta.r7(Where)";comment = "The Fifth Question: Place and Domain. The inquiry into spatial context—where events occur, in what realm, and within what boundaries of possibility.";color = darkgreen;];
    spark_5_meta_view_t [label = "Meta.r5(Who)";comment = "The Sixth Question: Agency and Identity. The inquiry into consciousness—who acts, who experiences, and through what identity existence knows itself.";color = darkgreen;];
    spark_3_meta_view_t [label = "Meta.r3(IN)";comment = "Thesis: The Principle of Potential. The fundamental act of reception—the opening, the invitation, the womb of possibility that receives all that might become.";color = darkblue;];
    spark_6_meta_view_t [label = "Meta.r6(OUT)";comment = "Antithesis: The Principle of Manifestation. The fundamental act of emission—the expression, the creation, the birth of actuality from the womb of potential.";color = darkblue;];
    spark_9_meta_view_t [label = "Meta.r9(REC)";comment = "Synthesis: The Principle of Relationship. The fundamental act of connection—the recursion, the binding, the eternal dance that weaves IN and OUT into coherent wholes.";color = darkblue;];
    spark_a_meta_view_t [label = "Meta.receive(Whence)";comment = "Potential: The Source of All Questions. The undifferentiated wellspring from which all possible patterns and interrogatives emerge—the formless origin of form itself.";shape = invtriangle;color = darkred;];
    spark_b_meta_view_t [label = "Meta.send(Whither)";comment = "Manifest: The Resolution of Inquiry. The emission of fully answered questions—patterns that have found their place in the cosmic tapestry, ready to participate in reality's dance.";shape = triangle;color = darkred;];
